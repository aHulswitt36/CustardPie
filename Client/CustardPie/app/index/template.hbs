	</span> -->
  <div id="mainPicSlider">
    {{#slick-slider autoplay=true autoplayspeed=6500 arrows=false}}
    <span id="bandPicBg" style="background-color: #ffff">
      <img src='assets/images/band.jpg' alt="Custard Pie" class='mainPic' />
    </span>
    <span id="bandPicBg" style="background-color: #ffff">
      <img src='assets/images/collage.jpg' alt="Custard Pie" class='mainPic' />
    </span>
    {{/slick-slider}}
  </div>

  <div style="position: relative;">
    <div id="upcomingShows">
      <h3>Upcoming Shows</h3>
      <ul>
        {{#each model as |event|}}
        <li>{{moment-format event.date "MM/DD"}} {{event.venue}}</li>
        {{/each}}
      </ul>
      {{#link-to 'schedule'}}<span class="seeMoreShows">See More Shows</span>{{/link-to}}
    </div>

    <div id="upcomingShowsMobile">
      {{#link-to 'schedule' class='mobileLink' }}<span class="mobileText">Come See Us Play <span class="glyphicon glyphicon-chevron-right"></span></span>
      {{/link-to}}
    </div>
    <div id="playlistMobile">
      {{#link-to 'playlist' class='mobileLink' }}<span class="mobileText">What We Play <span class="glyphicon glyphicon-chevron-right"></span></span>
      {{/link-to}}
    </div>
